jupiterToysCartPage_price_td_xpath=//td[contains(text(),'%s')]/parent::tr//td[2]
jupiterToysCartPage_total_td_xpath=//td[contains(text(),'%s')]/parent::tr//input[@name='quantity']/parent::td/following-sibling::td[@class]
jupiterToysCartPage_cartItems_span_xpath=//li[@id='nav-cart']/a//span