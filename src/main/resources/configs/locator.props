#jupiterToysShopPage_FunnyCowBuy_button_xpath=//h4[text()='Funny Cow']/parent::div//a

# --- Locators of Jupiter Toys cart Page ---

jupiterToysCartPage_items_tr_xpath=//tr[@class='cart-item ng-scope']
jupiterToysCartPage_checkout_href_xpath=//a[@href='#/checkout']
jupiterToysCartPage_shoppingLink_href_xpath=//a[contains(text(),'Start Shopping')]
jupiterToysCartPage_item_td_xpath=//td[contains(text(),'%s')]
